{
  "dart.lineLength": 80,
  "editor.formatOnSave": true,
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "files.exclude": {
    "out": false,
    ".dart_tool": true
  },
  "search.exclude": {
    "out": true,
    ".dart_tool": true
  },
  "testing.automaticallyOpenPeekView": "never",
  "vscode-test-explorer.enable": false
}
