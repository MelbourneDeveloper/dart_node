{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "dart: compile"
    },
    {
      "name": "Dart Tests",
      "type": "dart",
      "request": "launch",
      "program": "test/",
      "cwd": "${workspaceFolder}"
    }
  ]
}
