{
  "name": "dart-react-native-tests",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:tests": "dart compile js test/ui_test.dart -o test/dist/ui_test.js"
  },
  "devDependencies": {
    "@react-native/babel-preset": "^0.80.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-native": "^0.76.3",
    "react-test-renderer": "^18.3.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-native-community|react-native-.*)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "verbose": true
  }
}
